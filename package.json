{
  "name": "labofolio",
  "version": "1.0.0",
  "description": "foway's blog",
  "author": "foway0",
  "license": "ISC",
  "homepage": "https://github.com/foway0/labofolio#readme",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "DEBUG_COLORS=true DEBUG=labofolio:* nodemon -e ts,yaml -w src -x 'ts-node ./src/index.ts'",
    "build": "tsc && run-s build:yaml",
    "build:yaml": "cp -r src/api_specs dist",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "test": "ava -v",
    "coverage": "nyc --reporter html ava"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-openapi-validator": "3.7.0-alpha.3",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "@types/sinon": "^7.5.1",
    "@types/sinon-express-mock": "^1.3.7",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "ava": "^3.3.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foway0/labofolio.git"
  },
  "bugs": {
    "url": "https://github.com/foway0/labofolio/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
